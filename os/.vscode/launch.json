    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb-multiarch) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/riscv64gc-unknown-none-elf/release/os",
            "cwd": "${workspaceFolder}",
            "MIMode": "gdb",
            "miDebuggerPath": "gdb-multiarch",
            "miDebuggerArgs": "-ex 'set arch riscv:rv64'",
            "miDebuggerServerAddress": "localhost:1234"
        }
    ]
  }